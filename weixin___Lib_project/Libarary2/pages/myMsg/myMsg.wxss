page {
  background: #F0F0F0;
  background-size: 100% 100%;
  width: 100%;
  height: 100%;
  font-family: "微软雅黑";
}

.header {
  height: 118rpx;
  width: 100%;
  background: #333;
  position: fixed;
  z-index: 2;
}

.header-return {
  width: 70rpx;
  height: 70rpx;
  position: relative;
  left: 26rpx;
  top: 10.5px;
}

.header-title {
  color: #148cca;
  position: relative;
  left: 245rpx;
}

.standAplace {
  height: 120rpx;
}

.list{
  width: 650rpx;
  height: 420rpx;
  margin-left: 35rpx;
  margin-top: 40rpx;
  margin-bottom: 120rpx;
  background: white;
  padding: 20rpx;
}

.list-icon{
  width: 650rpx;
  height: 420rpx;
}

.mask{
  position: relative;
  background:  rgba(0,0,0,0.5);
  height: 90rpx;
  width: 650rpx;
  bottom: 98rpx;
  color: white;
  line-height: 90rpx;
  text-align: center;
  font-size: 31rpx;
}

.list-time{
  width: 280rpx;
  height: 43rpx;
  line-height: 43rpx;
  font-size: 28rpx;
  color: white;
  text-align: center;
  margin: auto;
  border-radius: 8rpx;
  background: lightgray;
  position: relative;
  bottom: 40rpx;
}