page {
  width: 100%;
  height: 100%;
  overflow: scroll;
  box-sizing: border-box;
  background: url("http://www.siliangjiadan.cn/img/timg.jpg") fixed;
  background-size: 100% 100%;
  font-family: "微软雅黑";
}

.mask {
  width: 100%;
  height: 100%;
  background: #fff;
  opacity: 0.5;
  position: absolute;
  position: fixed;
  z-index: -1;
}

.header {
  height: 118rpx;
  width: 100%;
  background: #333;
  position: fixed;
  z-index: 1;
}

.header-return {
  width: 70rpx;
  height: 70rpx;
  position: relative;
  left: 26rpx;
  top: 10.5px;
}

.header-title {
  color: #148cca;
  position: relative;
  left: 203rpx;
}

.header-order {
  color: #148cca;
  position: relative;
  left: 350rpx;
}

.standAplace {
  height: 120rpx;
}

.section{
   background:lightgray;
   margin:20rpx;
   padding:20rpx
}